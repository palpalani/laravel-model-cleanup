{
    "name": "spatie/laravel-model-cleanup",
    "description": "This package deletes unneeded records in a database.",
    "keywords": [
        "spatie",
        "laravel-model-cleanup"
    ],
    "homepage": "https://github.com/spatie/laravel-model-cleanup",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^8.2",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0|^11.0",
        "orchestra/testbench":"^9.0.4",
        "mockery/mockery": "^1.0",
        "spatie/test-time": "^1.2",
        "vimeo/psalm": "^5.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\ModelCleanup\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/pest"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\ModelCleanup\\ModelCleanupServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
